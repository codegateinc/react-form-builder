{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["es2020"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "rootDir": ".",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "baseUrl": "src"
  },
  "exclude": ["dist"]
}
